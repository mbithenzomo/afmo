###  http://purl.org/afmo#AgeGroup
:AgeGroup rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:oneOf ( :Above74
                                            :Below60
                                            <http://purl.org/afmo#60-64>
                                            <http://purl.org/afmo#65-74>
                                          )
                              ] ;
          rdfs:subClassOf :DemographicFactor ;
          rdfs:comment """Age group ranges were selected based on established AF clinical risk scores, namely HARMS-AF (Segan et al. (2023)), which assigns 1 point for age between 60 and 64 and 2 points for age above 64, and CH2EST (Alonso et al. (2013)), which assigns 2 points for age above 74.  

- Below 60
- 60 - 64
- 65 - 74
- Above 74""" ;
          rdfs:label "Age group" .


