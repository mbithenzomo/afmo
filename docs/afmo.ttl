                        rdfs:range :AlcoholConsumptionLevel ;
                        rdfs:comment "The relationship between a monitored person and their alcohol misuse status (present or absent)." ;
                        rdfs:label "has alcohol misuse status" .

[ rdf:type owl:AllDisjointProperties ;
   owl:members ( :hasAlcoholMisuseStatus
               )
 ] .


