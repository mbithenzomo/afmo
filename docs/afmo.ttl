###  http://purl.org/afmo#hasAgeGroup
:hasAgeGroup rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :isAgeGroupOf ;
             rdfs:domain :MonitoredPerson ;
             rdfs:range :AgeGroup ;
             <http://www.geneontology.org/formats/oboInOwl#hasDbXref> "saref4wear:wearerAgeRange" ;
             rdfs:comment "The relationship between a monitored person and their age group." ;
             rdfs:label "has age group" .

[ rdf:type owl:AllDisjointProperties ;
   owl:members ( :hasAgeGroup
               )
 ] .


