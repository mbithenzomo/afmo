    <!-- http://purl.org/afmo#AgeGroup -->


    <owl:Class rdf:about="http://purl.org/afmo#AgeGroup">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/afmo#Above74"/>
                    <rdf:Description rdf:about="http://purl.org/afmo#Below60"/>
                    <rdf:Description rdf:about="http://purl.org/afmo#60-64"/>
                    <rdf:Description rdf:about="http://purl.org/afmo#65-74"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/afmo#DemographicFactor"/>
        <rdfs:comment>Age group ranges were selected based on established AF clinical risk scores, namely HARMS-AF (Segan et al. (2023)), which assigns 1 point for age between 60 and 64 and 2 points for age above 64, and CH2EST (Alonso et al. (2013)), which assigns 2 points for age above 74.  

- Below 60
- 60 - 64
- 65 - 74
- Above 74</rdfs:comment>
        <rdfs:label>Age group</rdfs:label>
    </owl:Class>
    


