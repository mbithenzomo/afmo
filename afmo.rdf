    <!-- http://purl.org/afmo#PPG -->

    <owl:Class rdf:about="http://purl.org/afmo#PPG">
        <rdfs:subClassOf rdf:resource="http://purl.org/afmo#Biosignal"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/afmo#isRelatedToMeasurement"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/afmo#PPGMeasurement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <oboInOwl:hasDbXref>UMLS:C0162599</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym>Photoplethysmography</oboInOwl:hasExactSynonym>
        <rdfs:comment>Plethysmographic determination in which the intensity of light reflected from the skin surface and the red cells below is measured to determine the blood volume of the respective area. There are two types, transmission and reflectance.</rdfs:comment>
        <rdfs:label>PPG</rdfs:label>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://purl.org/afmo#PPG"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
        <owl:annotatedTarget>Plethysmographic determination in which the intensity of light reflected from the skin surface and the red cells below is measured to determine the blood volume of the respective area. There are two types, transmission and reflectance.</owl:annotatedTarget>
